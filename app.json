{
  "expo": {
    "name": "nusatech",
    "slug": "nusatech",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jasonevan.cobanusatech"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8ec531e-177d-412e-b380-8f12bb3de38e"
      }
    },
    "splash": {
      "image": "./assets/images/Feather.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
